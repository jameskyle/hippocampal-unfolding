load hit489Gam


%
%  This doesn't work because of monotonicity requirements
%
%invHitachiGam = interp1(hitgam(:,1)',0:255,s);

invHit489Gam = zeros(1000,3);
steps = [0:999] / 1000;
for rgb=1:3
 for dac=1:(length(hitgam)-1)
  l = find((hit489Gam(dac,rgb) < steps) & (hit489Gam(dac+1,rgb) > steps));
  if l ~= []
   invHit489Gam(l,rgb) = dac * ones(length(l),1);
  end
 end
end

save hit489Gam hit489Gam invHit489Gam
